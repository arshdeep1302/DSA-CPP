#include <iostream>
#include <vector>
using namespace std;

/*
    Checks whether placing 'num' at position (row, col)
    is valid according to Sudoku rules.
*/
bool safe(vector<vector<char>>& board, int row, int col, char num) {

    // Check current row and column
    for (int i = 0; i < 9; i++) {
        if (board[row][i] == num) return false; // row check
        if (board[i][col] == num) return false; // column check
    }

    // Find starting index of 3x3 subgrid
    int sr = (row / 3) * 3;
    int sc = (col / 3) * 3;

    // Check 3x3 subgrid
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            if (board[sr + i][sc + j] == num)
                return false;
        }
    }

    // Safe to place the number
    return true;
}

/*
    Solves the Sudoku board using backtracking.
    Returns true if a solution exists.
*/
bool solve(vector<vector<char>>& board) {

    // Traverse the board to find an empty cell
    for (int i = 0; i < 9; i++) {
        for (int j = 0; j < 9; j++) {

            // If empty cell is found
            if (board[i][j] == '.') {

                // Try all possible digits
                for (char num = '1'; num <= '9'; num++) {

                    // Check if current number can be placed
                    if (safe(board, i, j, num)) {

                        // Place the number
                        board[i][j] = num;

                        // Recursively solve the remaining board
                        if (solve(board))
                            return true;

                        // Backtrack if solution not possible
                        board[i][j] = '.';
                    }
                }

                // No valid number found for this cell
                return false;
            }
        }
    }

    // All cells filled successfully
    return true;
}

int main() {
     vector<vector<char>> board = {
        {'5','3','.','.','7','.','.','.','.'},
        {'6','.','.','1','9','5','.','.','.'},
        {'.','9','8','.','.','.','.','6','.'},
        {'8','.','.','.','6','.','.','.','3'},
        {'4','.','.','8','.','3','.','.','1'},
        {'7','.','.','.','2','.','.','.','6'},
        {'.','6','.','.','.','.','2','8','.'},
        {'.','.','.','4','1','9','.','.','5'},
        {'.','.','.','.','8','.','.','7','9'}
    };

    solve(board);

    for (auto &row : board) {
        for (auto c : row) cout << c << " ";
        cout << endl;
    }
    return 0;
}
